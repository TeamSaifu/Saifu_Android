<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ze20.saifu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReportActivity"></activity>
        <activity android:name=".AddWishActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".ui.budget.IncomeSettings" />
        <activity android:name=".ui.budget.SpendSettings" />
        <activity android:name=".DataInputActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.budget.BudgetActivity" />
    </application>
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

</manifest>
